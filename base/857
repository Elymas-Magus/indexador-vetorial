	<minorsubject>FUCOSIDASE: an, me</minorsubject>
	<minorsubject>HUMAN</minorsubject>
	<minorsubject>ISOELECTRIC-FOCUSING</minorsubject>
	<minorsubject>SUPPORT-U-S-GOVT-P-H-S</minorsubject>

